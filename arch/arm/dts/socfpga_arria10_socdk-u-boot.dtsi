#include "socfpga_arria10-u-boot.dtsi"

/ {
	aliases {
		bootargs = "console=ttyS0,115200";
		i2c0 = &i2c1;
	};
};

&i2c1 {
	/* Max value set according to HW spec */
	i2c-scl-falling-time-ns = <300>;
};

&uart1 {
	u-boot,dm-pre-reloc;
};

&watchdog1 {
        u-boot,dm-pre-reloc;
};
