/ {
	compatible = "altr,socfpga-cyclone5", "altr,socfpga";

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&osc1 {
	clock-frequency = <50000000>;
};

&l4_main_clk {
	u-boot,dm-pre-reloc;
};

&watchdog0 {
	status = "disabled";
};

&uart0 {
	clock-frequency = <100000000>;
	u-boot,dm-pre-reloc;
};

&mmc0 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&qspi {
	u-boot,dm-pre-reloc;
	status = "okay";

	flash0: s25fl512s@0 {
		u-boot,dm-pre-reloc;
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spansion,s25fl512s", "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		page-size = <256>;
		block-size = <16>; /* 2^16, 64KB */
		read-delay = <4>;
		tshsl-ns = <50>;
		tsd2d-ns = <50>;
		tchsh-ns = <4>;
		tslch-ns = <4>;
	};
};

&i2c0 {
	status = "okay";
	clock-frequency = <100000>;

	atsha204a: atsha204a@64 {
		status = "okay";
		compatible = "atmel,atsha204a";
		reg = <0x64>;
	};
};

